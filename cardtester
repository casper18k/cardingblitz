ut.innerText += "\nInvalid AVS JSON format.";\n return;\n }\n\n cards.forEach(card => {\n domains.forEach(domain => {\n const result = \\n[Testing] Domain: ${domain}\\n - Card: ${card}\\n - AVS: ZIP ${avsData.zip}, Street ${avsData.street}\\n - 3D Secure: ${secure}\\n -> Simulated: Request sent.;\n output.innerText += result;\n });\n });\n }\n </script>\n</body>\n</html>"
